+import java.io.PrintStream;
+import java.net.Socket;
+
+
+public class Cliente {
+	
+		public static void main (String args[]){
+			Socket s = null;
+			PrintStream ps = null;
+		try{
+			String x = "Teste 123 edgar raivoso";
